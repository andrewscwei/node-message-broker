  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/parse-json": "^4.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
