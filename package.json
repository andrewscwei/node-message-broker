{
  "name": "@sybl/rpc",
  "version": "0.2.0",
  "description": "RPC factory for Sybl core",
  "main": "build/index.js",
  "private": true,
  "scripts": {
    "prepare": "npm run build",
    "clean": "rimraf build",
    "build": "npm run clean && tsc",
    "test": "NODE_ENV=test DEBUG=rpc* mocha --timeout 20000 --reporter spec --full-trace --colors --recursive --bail --exit $([ -z \"$npm_config_path\" ] && echo build/**/*.spec.js || echo $npm_config_path)",
    "mq": "docker run --name rpc -d --rm -p 5672:5672 rabbitmq:management",
    "mq:stop": "docker stop rpc-mq",
    "lint": "tslint \"src/**/*.ts\" --project tsconfig.json",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sybl/rpc.git"
  },
  "files": [
    "build"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sybl/rpc/issues"
  },
  "homepage": "https://github.com/sybl/rpc#readme",
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/mocha": "^5.2.5",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@types/amqplib": "^0.5.9",
    "@sindresorhus/is": "^0.13.0",
    "amqplib": "^0.5.2",
    "debug": "^4.1.0"
  },
  "types": "build/index.d.ts"
}
